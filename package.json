{
  "name": "webgl-as",
  "version": "1.0.0",
  "description": "A basic implementation of WebGL canvas written with AssemblyScript",
  "main": "index.js",
  "scripts": {
    "test": "node tests",
    "asbuild:untouched": "asc assembly/index.ts --target debug",
