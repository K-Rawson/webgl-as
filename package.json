    "WebAssembly",
    "WASM",
    "AS",
    "GameDev",
    "Graphics",
    "3D"
  ],
  "author": "Kara Rawson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZoeDreams/webgl-as/issues"
  },
  "homepage": "https://github.com/ZoeDreams/webgl-as#readme",
  "dependencies": {
    "@assemblyscript/loader": "^0.17.4"
  },
  "devDependencies": {
    "assemblyscript": "^0.17.4"
  }
}
